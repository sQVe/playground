{
  "name": "playground",
  "version": "1.0.0",
  "description": "Simple JavaScript playground",
  "author": "Oskar Grunning <oskargrunning@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/sQVe/playground#playground",
  "repository": "github:sQVe/playground",
  "bugs": "https://github.com/sQVe/playground/issues",
  "keywords": [
    "javascript",
    "playground",
    "repl"
  ],
  "engines": {
    "node": ">= 10"
  },
  "type": "module",
  "scripts": {
    "clean.js": "nodemon --signal SIGHUP boxes/clean.js",
    "clean.ts": "nodemon --exec 'ts-node' --signal SIGHUP boxes/clean.ts",
    "fp.js": "nodemon --signal SIGHUP boxes/fp.js -- --experimental-modules",
    "fp.ts": "nodemon --exec 'ts-node' --signal SIGHUP boxes/fp.ts",
    "lint": "eslint '{**/*,*}.{js,jsx,ts,tsx}'"
  },
  "devDependencies": {
    "@types/node": "^13.7.7",
    "@types/ramda": "^0.26.43",
    "@types/sanctuary": "^0.14.0",
    "@typescript-eslint/eslint-plugin": "~2.20.0",
    "@typescript-eslint/parser": "~2.20.0",
    "babel-eslint": "~10.0.0",
    "eslint": "~6.8.0",
    "eslint-config-sqve": "~2.6.0",
    "eslint-plugin-fp": "~2.3.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "~2.0.2",
    "prettier": "~1.19.0",
    "ts-node": "~8.6.2",
    "typescript": "~3.7.5"
  },
  "dependencies": {
    "crocks": "~0.12.0",
    "fluture": "~12.2.0",
    "fp-ts": "~2.5.3",
    "ramda": "~0.27.0",
    "sanctuary": "~3.0.0"
  }
}
