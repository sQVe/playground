{
  "name": "playground",
  "version": "1.0.0",
  "description": "Simple JavaScript playground",
  "author": "Oskar Grunning <oskargrunning@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/sQVe/playground#playground",
  "repository": "github:sQVe/playground",
  "bugs": "https://github.com/sQVe/playground/issues",
  "keywords": [
    "javascript",
    "playground",
    "repl"
  ],
  "engines": {
    "node": ">= 10"
  },
  "type": "module",
  "scripts": {
    "clean.js": "nodemon --signal SIGHUP --experimental-modules boxes/clean.js",
    "clean.ts": "nodemon --exec 'ts-node' --signal SIGHUP boxes/clean.ts",
    "fp.js": "nodemon --signal SIGHUP --experimental-modules boxes/fp.js",
    "fp.ts": "nodemon --exec 'ts-node' --signal SIGHUP boxes/fp.ts",
    "lint": "eslint '{**/*,*}.{js,jsx,ts,tsx}'"
  },
  "devDependencies": {
    "@types/node": "^13.13.4",
    "@types/ramda": "^0.27.14",
    "@types/sanctuary": "^0.14.0",
    "@typescript-eslint/eslint-plugin": "~2.31.0",
    "@typescript-eslint/parser": "~2.30.0",
    "babel-eslint": "~10.1.0",
    "eslint": "~6.8.0",
    "eslint-config-sqve": "~2.7.0",
    "eslint-plugin-fp": "~2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.0",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "~2.0.4",
    "prettier": "~2.0.5",
    "ts-node": "~8.10.2",
    "typescript": "~3.8.0"
  },
  "dependencies": {
    "crocks": "~0.12.0",
    "fluture": "~12.2.1",
    "fp-ts": "~2.7.1",
    "ramda": "~0.27.0",
    "sanctuary": "~3.0.0"
  }
}
