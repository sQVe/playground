{
  "name": "playground",
  "version": "1.1.0",
  "description": "Simple JavaScript playground",
  "author": "Oskar Grunning <oskargrunning@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/sQVe/playground#playground",
  "repository": "github:sQVe/playground",
  "bugs": "https://github.com/sQVe/playground/issues",
  "keywords": [
    "javascript",
    "playground",
    "repl"
  ],
  "engines": {
    "node": ">= 10"
  },
  "type": "module",
  "scripts": {
    "clean.js": "nodemon --signal SIGHUP --experimental-modules boxes/clean.js",
    "clean.ts": "ts-node-dev --respawn -r esm boxes/clean.ts",
    "fp.js": "nodemon --signal SIGHUP --experimental-modules boxes/fp.js",
    "fp.ts": "ts-node-dev --respawn -r esm boxes/fp.ts",
    "lint": "eslint '{**/*,*}.{js,jsx,ts,tsx}'"
  },
  "devDependencies": {
    "@types/node": "^15.6.0",
    "@types/ramda": "^0.27.40",
    "@types/sanctuary": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-sqve": "^2.15.11",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.0",
    "eslint-plugin-react": "^7.23.2",
    "nodemon": "^2.0.0",
    "prettier": "^2.3.0",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "crocks": "~0.12.0",
    "esm": "~3.2.25",
    "fluture": "~14.0.0",
    "fp-ts": "~2.10.5",
    "ramda": "~0.27.0",
    "sanctuary": "~3.1.0"
  }
}
