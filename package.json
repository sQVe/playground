{
  "name": "playground",
  "version": "1.1.0",
  "description": "Simple JavaScript playground",
  "author": "Oskar Grunning <oskargrunning@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/sQVe/playground#playground",
  "repository": "github:sQVe/playground",
  "bugs": "https://github.com/sQVe/playground/issues",
  "keywords": [
    "javascript",
    "playground",
    "repl"
  ],
  "engines": {
    "node": ">= 10"
  },
  "type": "module",
  "scripts": {
    "clean.js": "nodemon --signal SIGHUP --experimental-modules boxes/clean.js",
    "clean.ts": "ts-node-dev --respawn -r esm boxes/clean.ts",
    "fp.js": "nodemon --signal SIGHUP --experimental-modules boxes/fp.js",
    "fp.ts": "ts-node-dev --respawn -r esm boxes/fp.ts",
    "lint": "eslint '{**/*,*}.{js,jsx,ts,tsx}'"
  },
  "devDependencies": {
    "@types/node": "^14.14.13",
    "@types/ramda": "^0.27.33",
    "@types/sanctuary": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.9.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.15.0",
    "eslint-config-sqve": "^2.15.11",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-standard": "^4.1.0",
    "nodemon": "^2.0.0",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "crocks": "~0.12.0",
    "esm": "~3.2.25",
    "fluture": "~13.0.0",
    "fp-ts": "~2.8.6",
    "ramda": "~0.27.0",
    "sanctuary": "~3.1.0"
  }
}
